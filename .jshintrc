{
  "globalstrict": true,
  "overrides": {
    "*_test.js": {
      "globals": {
        "module": false,
        "inject": false
      },
      "jasmine": true
    },
    "e2e-tests/**/*": {
      "globals": {
	"browser": false,
	"by": false,
	"element": false
      },
      "esversion": 6,
      "jasmine": true,
      "node": true
    },
    "karma.conf.js": {
      "node": true
    },
    "src/**/*": {
      "globals": {
        "angular": false
      },
      "node": true
    },
    "static/**/*": {
      "globals": {
        "angular": false,
	"inject": false
      }
    },
    "utilities/**/*": {
      "node": true
    },
    "webpack.config.js": {
      "node": true
    }
  }
}