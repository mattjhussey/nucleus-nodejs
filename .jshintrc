{
  "overrides": {
    "karma.conf.js": {
      "node": true
    },
    "e2e-tests/protractor.conf.js": {
      "esversion": 6,
      "node": true
    },
    "e2e-tests/**/*.spec.js": {
      "globals": {
        "browser": false,
        "by": false,
        "element": false
      },
      "mocha": true,
      "node": true
    },
    "gulpfile.js": {
      "esversion": 6,
      "node": true
    },
    "src/**/*.js": {
      "node": true
    },
    "src/**/*.spec.js": {
      "globals": {
        "angular": false,
        "inject": false,
        "MouseEvent": false
      },
      "mocha": true
    },
    "webpack.config.js": {
      "node": true
    }
  },
  "strict": "global"
}