{
  "opts": {
    "destination": "./docs/",
    "package": "./package.json",
    "pedantic": true,
    "private": true,
    "recurse": true
  },
  "plugins": [],
  "source": {
    "exclude": [
      "./coverage",
      "./docs",
      "./node_modules",
      "./public"
    ],
    "excludePattern": "(^|\\/|\\\\)_",
    "includePattern": ".+\\.js(doc)?$"
  },
  "tags": {
    "allowUnknownTags": false
  },
  "templates": {
    "cleverLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "monospaceLinks": false
  }
}
