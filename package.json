{
  "description": "A starter project for AngularJS",
  "dependencies": {
    "angular": "1.5.5",
    "angular-loader": "1.5.5",
    "angular-mocks": "1.5.5",
    "angular-route": "1.5.5",
    "html5-boilerplate": "h5bp/html5-boilerplate#b5d6e7b"
  },
  "devDependencies": {
    "copy-dir": "^0.0.8",
    "css-loader": "0.23.1",
    "csslint": "0.10.0",
    "del-cli": "0.2.0",
    "express": "^4.13.4",
    "htmllint-cli": "htmllint/htmllint-cli#adf7a8d",
    "http-server": "0.9.0",
    "jasmine-core": "^2.3.4",
    "jscs": "3.0.3",
    "jsdoc": "3.4.0",
    "jshint": "2.9.2",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.0.0",
    "protractor": "3.3.0",
    "pug-lint": "2.1.7",
    "rimraf": "2.5.2",
    "shelljs": "0.7.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1"
  },
  "license": "MIT",
  "name": "angular-seed",
  "private": true,
  "repository": "https://github.com/angular/angular-seed",
  "scripts": {

    "precompile": "npm run compile:clean",
    "compile": "node utilities/copy-files.js static app && npm run webpack",

    "precompile:clean": "npm install",
    "compile:clean": "rimraf app",

    "precsslint": "npm install",
    "csslint": "csslint .",

    "documentation": "npm run jsdoc",

    "prehtmllint": "npm install",
    "htmllint": "htmllint static/**/*.html",
    
    "preinstall": "npm prune",

    "prejscs": "npm install",
    "jscs": "jscs .",

    "prejsdoc": "npm run jsdoc:generate",
    "jsdoc": "docs\\jsdoc\\index.html",

    "prejsdoc:clean": "npm install",
    "jsdoc:clean": "rimraf ./docs/jsdoc",

    "prejsdoc:generate": "npm run jsdoc:clean && npm install",
    "jsdoc:generate": "jsdoc --configure .jsdoc.conf.json .",

    "prejshint": "npm install",
    "jshint": "jshint .",

    "prekarma": "npm install && npm run compile",
    "karma": "karma start karma.conf.js --single-run",

    "lint": "npm run csslint && npm run jscs && npm run jshint && npm run puglint",

    "preprotractor": "npm install && npm run update-webdriver && npm run compile",
    "protractor": "protractor e2e-tests/protractor.conf.js",

    "prepuglint": "npm install",
    "puglint": "pug-lint .",

    "prestart": "npm install && npm run compile",
    "start": "http-server -a localhost -p 8000 -c-1",

    "test": "npm run karma && npm run protractor",

    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",

    "prewebpack": "npm install && npm run webpack:clean",
    "webpack": "webpack --progress --colors",

    "prewebpack:clean": "npm install",
    "webpack:clean": "del-cli app/bundle.js",

    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "version": "0.0.0"
}
